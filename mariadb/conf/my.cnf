[mysqld]
sql-mode="NO_ENGINE_SUBSTITUTION"
skip-host-cache
skip-name-resolve
innodb_use_native_aio=0
innodb_flush_method=O_DSYNC
log_bin=ON
default-authentication-plugin=mysql_native_password

[mysqld_safe]
skip_log_error
syslog
sql-mode="NO_ENGINE_SUBSTITUTION"
skip-host-cache
skip-name-resolve
innodb_use_native_aio=0
innodb_flush_method=O_DSYNC
log_bin=ON
default-authentication-plugin=mysql_native_password
    
